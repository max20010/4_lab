#include "stdafx.h"
#include "iostream"
using namespace std;


int main()
{
	setlocale(LC_ALL, "ru");
	int x,max_=0,min_=0,n;
	cin >> x;
	int *a = new int[x];
	for (int i = 0; i < x; i++){
		cin >> a[i];
	}
	int min = a[0], max = a[0];
	for (int i = 0; i < x; i++){
		if (max < a[i]){
			max = a[i];
			max_ = i; 
		}
		if (min > a[i]){
			min = a[i];
			min_ = i;
		}
	}
	n = a[min_];
	a[min_] = a[max_];
	a[max_] = n;
	for (int i = 0; i < x;i++){
		cout << a[i] << endl;
	}
	delete[] a;
	return 0;
}

