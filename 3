#include "stdafx.h"
#include "iostream"
using namespace std;


int main()
{
	setlocale(LC_ALL, "ru");
	int x, b, n=0,n1=0;
	cout << "Кол-во чисел в массиве: ";
	cin >> x;
	cout << "Удалить числа больше: ";
	cin >> b;
	int *a = new int[x];
	for (int i = 0; i < x; i++) {
		cin >> a[i];
	}
	for (int i = 0; i < x; i++) {
		if (a[i]>=b) {
			n++;
		}
	}
	int *a_ = new int[n];
	for (int i = 0; i < x; i++) {
		if (a[i]>=b) {
			a_[n1] = a[i];
			n1++;
		}
	}
	for (int i = 0; i < n;i++) {
		cout << a_[i] << "\n";
	}
	delete[] a;
	delete[] a_;
	return 0;
}
